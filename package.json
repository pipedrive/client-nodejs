{
  "name": "pipedrive",
  "version": "11.0.0",
  "description": "Pipedrive REST client for NodeJS",
  "keywords": [
    "pipedrive",
    "CRM",
    "sales",
    "contacts",
    "customers",
    "deals",
    "pipeline",
    "sales pipeline"
  ],
  "license": "MIT",
  "homepage": "https://github.com/pipedrive/client-nodejs",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=8.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/pipedrive/client-nodejs.git"
  },
  "scripts": {
    "lint": "eslint lib",
    "test": "echo \"Temporarily skipping integration tests\" && exit 0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^7.1.1"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "request": "^2.88.2"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "commonjs": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "no-underscore-dangle": 0,
      "strict": 0,
      "prefer-rest-params": 0
    }
  }
}
